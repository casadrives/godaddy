# API Configuration
REACT_APP_API_URL=https://api.luxembourgtaxi.com
REACT_APP_API_VERSION=v1

# Maps Configuration
REACT_APP_MAPBOX_TOKEN=your_mapbox_token_here

# Authentication
REACT_APP_AUTH0_DOMAIN=your_auth0_domain
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=your_auth0_audience

# GPS Configuration
REACT_APP_GPS_UPDATE_INTERVAL=5000
REACT_APP_GPS_TIMEOUT=10000

# Error Reporting
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Region Settings
REACT_APP_DEFAULT_TIMEZONE=Europe/Luxembourg
REACT_APP_DEFAULT_LOCALE=fr-LU
REACT_APP_SUPPORTED_LANGUAGES=en,fr,de,lu
